{
    "compilerOptions": {
        "outDir": "dist",
        "module": "commonjs",
        "noEmitOnError": true,
        "noImplicitAny": true,
        "removeComments": true,
        "strictNullChecks": true,
        "experimentalDecorators": true,
        "moduleResolution":"node",
        "esModuleInterop": true
    },
    "include": ["src/**/*"]
}